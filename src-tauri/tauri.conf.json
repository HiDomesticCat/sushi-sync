{
  "productName": "sushi-sync",
  "version": "0.1.0",
  "identifier": "com.sushi-sync.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../build"
  },
  "app": {
    "windows": [
      {
        "title": "Sushi Sync",
        "width": 1200,
        "height": 800
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "./icons/32x32.png",
      "./icons/128x128.png",
      "./icons/128x128@2x.png",
      "./icons/icon.icns",
      "./icons/icon.ico"
    ]
  }
}
